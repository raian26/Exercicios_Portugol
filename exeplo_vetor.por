programa
{
	
	funcao inicio()
	{
		cadeia nomes[5]
		para(inteiro i = 0; i<5; i++){
			escreva("Digite o nome na posição " ,i+1, "°:\n" )
			leia(nomes[i])
		}
	para(inteiro i = 0; i<5; i++){
		escreva(nomes[i], "\n")
	}
	}
}
